#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int a, b, c;
	int x;
	printf("输入三个数：\n");
	scanf("%d%d%d", &a, &b, &c);
	if (a > b) 
	{
		x = a;
		a = b;
		b = x;
	}
	if (a > c)
	{
		x = a;
		a = c;
		c = x;
	}
	if (b > c) 
	{
		x = b;
		b = c;
		c = x;
	}
	printf("%d %d %d", c, b, a);
	return 0;
}
